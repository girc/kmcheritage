{
  "name": "nenad/yii2-advanced-template",
  "description": "Improved Yii 2 Advanced Application Template By Nenad Zivkovic",
  "keywords": [
    "yii2",
    "framework",
    "advanced",
    "improved",
    "application template",
    "nenad"
  ],
  "type": "project",
  "license": "BSD-3-Clause",
  "support": {
    "source": "https://github.com/girc/kmc"
  },
  "minimum-stability": "stable",
  "require": {
    "php": ">=5.4.0",
    "yiisoft/yii2": "*",
    "yiisoft/yii2-bootstrap": "*",
    "yiisoft/yii2-swiftmailer": "*",
    "nenad/yii2-password-strength": "*",
    "mihaildev/yii2-ckeditor": "*",
    "yiisoft/yii2-imagine": "~2.0",
    "ramshresh/yii2-gallery-manager": "dev-master"
  },
  "require-dev": {
    "yiisoft/yii2-codeception": "*",
    "yiisoft/yii2-debug": "*",
    "yiisoft/yii2-gii": "*",
    "yiisoft/yii2-faker": "*",
    "codeception/specify": "*",
    "codeception/verify": "*",
    "lsolesen/pel": "dev-master"
  },
  "config": {
    "vendor-dir": "_protected/vendor",
    "process-timeout": 1800
  },
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "_protected/vendor/npm",
      "bower-asset-library": "_protected/vendor/bower"
    }
  },
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/lsolesen/pel.git"
    },
    {
      "type": "git",
      "url": "https://github.com/ramshresh/yii2-gallery-manager.git"
    }
  ]
}
